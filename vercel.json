{
	"version": 2,
	"builds": [
		{
			"src": "./backend/server.js",
			"use": "@vercel/node"
		},
		{
			"src": "./frontend/build",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/.*",
			"dest": "server.js"
		}
	],
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/backend/server.js"
		}
	]
}
